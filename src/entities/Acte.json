      "type": "string",
      "description": "Num\u00e9ro unique de l'acte"
    },
    "date_bpd": {
      "type": "string",
      "format": "date",
      "description": "Date BPD de l'acte"
    },
    "type_acte": {
      "type": "string",
      "enum": [
        "Vente",
        "Donation",
        "Succession",
        "Hypoth\u00e8que",
        "Pr\u00eat",
        "\u00c9change",
        "Bail",
        "Servitude",
        "Autre"
