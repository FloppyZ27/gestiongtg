{
  "name": "Acte",
  "type": "object",
  "properties": {
    "numero_acte": {
      "type": "string",
      "description": "Num\u00e9ro unique de l'acte"
    },
    "numeros_actes_anterieurs": {
      "type": "array",
      "description": "Liste des num\u00e9ros d'actes ant\u00e9rieurs",
      "items": {
        "type": "string"
      }
    },
    "date_bpd": {
      "type": "string",
      "format": "date",
      "description": "Date BPD de l'acte"
    },
    "type_acte": {
      "type": "string",
      "enum": [
        "Vente",
        "Cession",
        "Donation",
        "D\u00e9claration de Transmission",
        "Jugement",
        "Rectification",
        "Retrocession",
        "Servitude",
        "Bornage"
      ],
      "description": "Type d'acte notari\u00e9"
    },
    "type_servitude": {
      "type": "string",
      "enum": [
        "\u00c9lectrique",
        "T\u00e9l\u00e9phonique",
        "Passage",
        "Aqueduc",
        "\u00c9gout",
        "Industrielle",
        "Vue",
        "Mitoyennet\u00e9",
        "Tol\u00e9rance",
        "Baignage",
        "Autre"
      ],
      "description": "Type de servitude (si type_acte est Servitude)"
    },
    "circonscription_fonciere": {
      "type": "string",
      "enum": [
        "Lac-Saint-Jean-Est",
        "Lac-Saint-Jean-Ouest",
        "Chicoutimi"
      ],
      "description": "Circonscription fonci\u00e8re"
    },
    "notaire": {
      "type": "string",
      "description": "Nom du notaire responsable"
    },
    "document_pdf_url": {
      "type": "string",
      "description": "URL du document PDF upload\u00e9"
    },
    "vendeurs": {
      "type": "array",
      "description": "Liste des vendeurs",
      "items": {
        "type": "object",
        "properties": {
          "nom": {
            "type": "string"
          },
          "prenom": {
            "type": "string"
          }
        }
      }
    },
    "acheteurs": {
      "type": "array",
      "description": "Liste des acheteurs",
      "items": {
        "type": "object",
        "properties": {
          "nom": {
            "type": "string"
          },
          "prenom": {
            "type": "string"
          }
        }
      }
    }
  },
  "required": [
    "numero_acte",
    "date_bpd",
    "type_acte",
    "circonscription_fonciere",
    "notaire"
  ]
}