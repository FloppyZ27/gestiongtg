    },
    "notaire": {
      "type": "string",
      "description": "Nom du notaire responsable"
    },
    "vendeurs": {
      "type": "array",
      "description": "Liste des vendeurs",
      "items": {
        "type": "object",
        "properties": {
          "nom": {
            "type": "string"
          },
          "prenom": {
            "type": "string"
          },
          "adresse": {
            "type": "string"
          }
        }
      }
    },
    "acheteurs": {
      "type": "array",
      "description": "Liste des acheteurs",
      "items": {
        "type": "object",
        "properties": {
          "nom": {
            "type": "string"
          },
          "prenom": {
            "type": "string"
          },
          "adresse": {
            "type": "string"
          }
        }
      }
    }
  },
  "required": [
    "numero_acte",
    "date_bpd",
    "type_acte",
    "notaire"
  ]
}